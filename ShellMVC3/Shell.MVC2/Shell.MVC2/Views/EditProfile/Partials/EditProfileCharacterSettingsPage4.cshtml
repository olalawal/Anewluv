    <div id="log"></div>
     <div class="Describe_what_makes_aboutEdit">
				
					<p class="lastNode">What type of humor do you have and what are your political incliations if any, also use the "You" section to choose what you look for in a match Humor and Political inclination wise
				</p>
	</div>
			
			<div class="clearFloat"></div>
         
        @using (Html.BeginForm("EditProfileCharacterSettings", "EditProfile", new { @ViewIndex = Constants.CharacterSettingsPage4, @fulledit = Model.IsFullEditing }, FormMethod.Post))
           { 

           <div id="contentforpartial">
      
      		<div class="MeMyQSElifestyle">				
					<p class="lastNode">Me(Humor and Political Inclination) 
				</p>
			</div>
			<div class="clearFloat"></div>
            <div id="MiddleCenterAlign_win_MyQSElifestyle" >
			<div id="hasKidsMeComboBox_win_MyQSElifestyle">
              @(Html.Telerik().ComboBoxFor(model => Model.CharacterSettings.HumorID  )
                                                            .Name("Model.CharacterSettings.HumorID")
                    .HtmlAttributes(new { style = string.Format("width: 125px") })
                                   .BindTo(Humors) 
                     .AutoFill(true)
)
            </div> 
		  <div id="smokeMeComboBox_win_MyQSElifestyle">          
         
       </div> 

			<div id="maritalStatusMeComboBox_win_MyQSElifestyle">
                  @(Html.Telerik().ComboBoxFor(model => Model.CharacterSettings.PoliticalViewID  )
                                                                                  .Name("Model.CharacterSettings.PoliticalViewID")
                    .HtmlAttributes(new { style = string.Format("width: 125px") })
                       .BindTo(PoliticalViews ) 
                     .AutoFill(true)
)
            </div> 
                      

             </div> 
         
           
                 <div class="clearFloat"></div>
			<div class="YouMyQSElifestyle">				
					<p class="lastNode">You (What I am looking for in a Match)
				</p>
			</div>
							<div class="clearFloat"></div>
			
