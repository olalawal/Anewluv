            <div class="MiddleCheckBoxHeaderEdit">
					<p class="lastNode">Your Humor Types
			</p>
		   </div>
           
   
            <table style="width: 95%"  class="CheckboxLabelText"  > 
                <tr> </tr> 
               @{ int cnt2 = 0; } 
               @foreach(var showme in Model.CharacterSearchSettings.humorlist)                   
                 { 
                  if (cnt2 == 5 || cnt2 == 10 || cnt2 == 15 || cnt2 == 20 || cnt2 == 25) 
                  {   <tr></tr> }
                    
                  
 
                      <td style="padding:2px;" >  
                      @if (showme.Selected) 
                          {     
                          <input type="checkbox" class="SelectedYourHumorIds" name="SelectedYourHumorIds" value="@showme.HumorID" checked="checked"  />   
                              
                           } else 
                          {         
                          <input type="checkbox" class ="SelectedYourHumorIds" name="SelectedYourHumorIds" value="@showme.HumorID"  />
                           }  
                        
                        @showme.HumorName
                       </td> 
                    
                     <!-- increment counter here only once -->
                       cnt2++  ;                
                        
                  
                    
                  }
                  
			 </table> 

	
</div> 

             				<div class="clearFloat"></div>
                              
                              <div class ="MiddleCheckFullWithBoxBodyEdit">
