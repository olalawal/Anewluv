@model IEnumerable<Dating.Server.Data.Models.photo>

<!DOCTYPE html>

<html>

<body>
    <p>
       Edit Photos
    </p>
    <table>
        <tr>
            <th>
                ProfileID
            </th>
              
              <th>Image </th>
            <th>
                Caption
            </th>
            <th>
                ProfileImageType
            </th>
            <th>
                Aproved
            </th>
            <th>
                PhotoDate
            </th>
            <th>
                PhotoReviewStatusID
            </th>
            <th>
                PhotoReviewerID
            </th>
            <th>
                PhotoReviewDate
            </th>

        </tr>
    
    @{ foreach (var item in Model) { 
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProfileID)
            </td>
            <td>
            <img  src="@Url.Action("GetGalleryImageByPhotoID", "Images", new { id = item.PhotoID},null)" alt="" width ="94" height ="94" />
	</td>
            <td>
                @Html.DisplayFor(modelItem => item.ImageCaption)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfileImageType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aproved)
                

            </td>

            <td>
                @Html.DisplayFor(modelItem => item.PhotoDate)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.PhotoReviewStatusID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhotoReviewerID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhotoReviewDate)
            </td>
            <td>
                @Html.ActionLink("Edit", "PhotoEdit", new { id=item.PhotoID }) |
               @* <%: Html.ActionLink("Details", "Details", new { id=item.PhotoID }) %> *@
            </td>
        </tr>
     } }
    
    </table>
</body>
</html>

