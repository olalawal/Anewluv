  <system.web>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>
  <system.serviceModel>

    <services>
      <service name="Shell.MVC2.Services.Dating.MembersMapperService">
        <endpoint address="Soap"
                  binding="basicHttpBinding"
                  contract="Shell.MVC2.Services.Contracts.IMembersMapperService"
                  name="soap"/>
        <endpoint address="Rest"
                  binding="webHttpBinding"
                  behaviorConfiguration="web"
                  contract="Shell.MVC2.Services.Contracts.IMembersMapperService"
                  name="rest"/>
      </service>
      <service name="Shell.MVC2.Services.Dating.MemberService">
        <endpoint address="Soap"
                  binding="basicHttpBinding"
                  contract="Shell.MVC2.Services.Contracts.IMemberService"
                  name="soap"/>
        <endpoint address="Rest"
                  binding="webHttpBinding"
                  behaviorConfiguration="web"
                  contract="Shell.MVC2.Services.Contracts.IMemberService"
                  name="rest"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
        <behavior name="web">
          <webHttp automaticFormatSelectionEnabled="true"
                   helpEnabled="true"
                   faultExceptionEnabled="true"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <serviceHostingEnvironment multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
 <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
